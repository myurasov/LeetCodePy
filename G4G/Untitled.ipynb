    "\"\"\"\n",
    "Input : n = 10\n",
    "Output : 8\n",
    "\n",
    "Input : n = 19\n",
    "Output : 16\n",
    "\n",
    "Input : n = 32\n",
    "Output : 32\n",
    "\"\"\"\n",
    "\n",
    "def highest2pow(n):\n",
    "    r = 1\n",
    "    while r <= n:\n",
    "        r = r << 1\n",
    "    return r >> 1\n",
    "\n",
    "print(highest2pow(33))\n",
    "print(highest2pow(32))\n",
    "print(highest2pow(3))"
